  "main": "index.js",
  "scripts": {
    "dev": "slidev",
    "build": "slidev build --base /repepe/",
    "build:single": "slidev build && npm run inline-build",
    "inline-build": "echo 'Build conclu√≠do. Verifique dist/index.html'",
    "deploy": "npm run build && gh-pages -d dist",
    "export": "slidev export",
    "export:pdf": "slidev export --dark --timeout 60000 --wait 1000 --wait-until load --per-slide",
    "export:pdf-light": "slidev export --timeout 60000 --wait 1000 --wait-until load --per-slide",
    "export:png": "slidev export --format png --dark --timeout 60000 --wait 1000",
    "export:pptx": "slidev export --format pptx --dark --timeout 60000 --wait 1000 --wait-until load --per-slide"
  },
