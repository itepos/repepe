    "playwright-chromium": "^1.57.0",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-svg-loader": "^5.1.0"
  },
  "dependencies": {
    "@ktym4a/slidev-theme-ktym4a": "^0.3.2",
